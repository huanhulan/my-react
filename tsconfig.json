{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "my-react/*": ["./my-react/*"]
    },
    "rootDir": "./src",
    "target": "esnext",
    "moduleResolution": "Node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["./src", "./my-react"],
  "exclude": []
}
