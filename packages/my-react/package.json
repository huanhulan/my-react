{
  "name": "my-react",
  "version": "0.0.1",
  "description": "Simple React implementation",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "umd:main": "dist/index.umd.js",
  "source": "./index.ts",
  "type": "module",
  "types": "dist/typings/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "production": "./dist/index.esm.js",
      "development": "./index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "ramda": "^0.27.1"
  },
  "scripts": {
    "build": "yarn cleanup && yarn build:umd & yarn build:esm & yarn build:cjs",
    "build:umd": "rollup --config --format=umd",
    "build:esm": "rollup --config",
    "build:cjs": "rollup --config --format=cjs",
    "cleanup": "echo \"Cleanup last build result\" && rm -rf ./dist",
    "lint": "eslint -c ./.eslintrc.json ./__tests__ ./src",
    "test": "jest",
    "tsc": "tsc --project tsconfig.json"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.14.5",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@shopify/jest-dom-mocks": "^3.0.6",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/ramda": "^0.27.44",
    "@types/requestidlecallback": "^0.3.4",
    "@types/uuid": "^8.3.1",
    "babel-jest": "^27.2.1",
    "eslint": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jest": "^25.0.1",
    "faker": "^5.5.3",
    "jest": "^27.2.1",
    "minimist": "^1.2.5",
    "node-fetch": "^3.0.0",
    "rollup": "^2.56.3",
    "typescript": "^4.3.5"
  }
}
